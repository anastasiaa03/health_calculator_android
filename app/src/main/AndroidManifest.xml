<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.healthcalculator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HealthCalculator">
        <activity
            android:name=".WelcomeActivity"
            android:exported="false" />
        <activity
            android:name=".ui.indexes.IndexBodyMassActivity"
            android:exported="false"
            android:label="@string/text_index_body_mass_short" />
        <activity
            android:name=".ui.indexes.IndexMovementActivityActivity"
            android:exported="false"
            android:label="@string/text_index_movement_activity_short" />
        <activity
            android:name=".ui.indexes.IndexEnduranceCoefficientActivity"
            android:exported="false"
            android:label="@string/text_index_endurance_coefficient_short" />
        <activity
            android:name=".ui.indexes.IndexRobinsonActivity"
            android:exported="false"
            android:label="@string/text_index_robinson_short" />
        <activity
            android:name=".ui.indexes.IndexVitalActivity"
            android:exported="false"
            android:label="@string/text_index_vital_short" />
        <activity
            android:name=".ui.indexes.IndexSkibinskiActivity"
            android:exported="false"
            android:label="@string/text_index_skibinski_short" />
        <activity
            android:name=".ui.indexes.IndexKerdoActivity"
            android:exported="false"
            android:label="@string/text_index_kerdo_short" />
        <activity
            android:name=".ui.indexes.IndexFunctionalChangesActivity"
            android:exported="false"
            android:label="@string/text_index_functional_changes_short" />
        <activity
            android:name=".ui.indexes.IndexResultsActivity"
            android:exported="false"
            android:label="@string/text_results" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>